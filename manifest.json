{
    "manifest_version": 3,
    "name": "mirror off",
    "version": "1.0",
    "description": "Отключить поворот камеры в google meet.",
    "permissions": ["activeTab"],
    "content_scripts": [
      {
        "matches": ["https://meet.google.com/*"],
        "css": ["styles.css"]
      }
    ],
    "action": {
        "default_icon": {
         "16": "icons/icon16.png",
         "48": "icons/icon48.png",
         "128": "icons/icon128.png"
    }
}}